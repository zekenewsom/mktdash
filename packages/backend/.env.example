PORT=3001

# Data provider behavior
# - live_with_fallback: try live APIs, fall back to mock data on failures
# - mock_only: skip live APIs, always return deterministic mock data
DATA_PROVIDER_MODE=live_with_fallback

# Upstream HTTP timeout in milliseconds
HTTP_TIMEOUT_MS=8000

# Optional: set your FRED key for live mode
FRED_API_KEY=

# Reserved for future market provider integration
ALPHAVANTAGE_API_KEY=
